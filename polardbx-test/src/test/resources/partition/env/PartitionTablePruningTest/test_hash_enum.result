CREATE TABLE `t_ubigint` (
	`a` bigint(20) UNSIGNED DEFAULT NULL,
	KEY `auto_shard_key_a` USING BTREE (`a`)
) ENGINE = InnoDB DEFAULT CHARSET = utf8mb4
PARTITION BY HASH (a) PARTITIONS 8;
