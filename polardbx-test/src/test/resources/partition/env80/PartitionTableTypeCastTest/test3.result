DROP TABLE IF EXISTS nation;
CREATE TABLE `nation` (
	`N_NATIONKEY` int(11) NOT NULL,
	`N_NAME` char(255) DEFAULT NULL,
	`N_REGIONKEY` int(11) DEFAULT NULL,
	`N_COMMENT` varchar(255) DEFAULT NULL,
	PRIMARY KEY (`N_NATIONKEY`)
) ENGINE = InnoDB DEFAULT CHARSET = utf8
PARTITION BY RANGE (`N_NATIONKEY`) (
	PARTITION p1 VALUES LESS THAN (100),
	PARTITION p2 VALUES LESS THAN (200),
	PARTITION p3 VALUES LESS THAN (300)
);
EXPLAIN SELECT *
FROM nation
WHERE 3 = 3.000;
Logical ExecutionPlan
Gather(concurrent=true)
  LogicalView(tables="nation[p1,p2,p3]", shardCount=3, sql="SELECT `N_NATIONKEY`, `N_NAME`, `N_REGIONKEY`, `N_COMMENT` FROM `nation` AS `nation` WHERE (? = ?)")
EXPLAIN SELECT *
FROM nation
WHERE N_NATIONKEY = 100
	AND N_NATIONKEY < 100.5;
Logical ExecutionPlan
PhyTableOperation(tables="nation[p2]", sql="SELECT `nation`.`N_NATIONKEY`, `nation`.`N_NAME`, `nation`.`N_REGIONKEY`, `nation`.`N_COMMENT` FROM ? AS `nation` WHERE ((`nation`.`N_NATIONKEY` = ?) AND (`nation`.`N_NATIONKEY` < ?))", params="100.5,100")
EXPLAIN SELECT *
FROM nation
WHERE N_NATIONKEY = 100
	AND N_NATIONKEY < 100.5
	AND N_NATIONKEY > 99.12;
Logical ExecutionPlan
PhyTableOperation(tables="nation[p2]", sql="SELECT `nation`.`N_NATIONKEY`, `nation`.`N_NAME`, `nation`.`N_REGIONKEY`, `nation`.`N_COMMENT` FROM ? AS `nation` WHERE ((`nation`.`N_NATIONKEY` = ?) AND (`nation`.`N_NATIONKEY` < ?) AND (`nation`.`N_NATIONKEY` > ?))", params="100.5,100,99.12")